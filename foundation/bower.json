{
  "name": "foundation-sites-template",
  "version": "1.0.0",
  "authors": [
    "ZURB <foundation@zurb.com>"
  ],
  "description": "Basic template for a new Foundation for Sites project.",
  "main": "index.html",
  "license": "MIT",
  "homepage": "http://foundation.zurb.com",
  "dependencies": {
    "foundation-sites": "latest",
    "motion-ui": "~1.2.2",
    "plyr": "^2.0.10",
    "rangetouch": "^0.0.9",
    "fontawesome": "~4.2.0"    
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "private": true,
  "overrides": {
    "foundation-sites": {
      "main": [
        "dist/foundation.min.js",
        "dist/plugins/foundation.reveal.js"
      ]
    },
    "plyr": {
      "main": [
        "src/js/plyr.js"
      ]
    },
    "rangetouch": {
      "main": [
        "dist/rangetouch.js"
      ]
    }   
  }
}
